<!DOCTYPE idea-plugin PUBLIC "Plugin/DTD"
		"http://plugins.intellij.net/plugin.dtd">
<idea-plugin version="2">

	<name>ProjectCommandLauncher</name>
	<id>ProjectCommandLauncher</id>
	<version>0.1</version>

	<description><![CDATA[

			]]>
	</description>

	<category>Editor</category>

	<vendor email="vojta.krasa@gmail.com">Vojtech Krasa</vendor>

	<idea-version since-build="110.000" until-build=""/>

	<change-notes><![CDATA[

			]]></change-notes>


	<actions>
		<action id="RunAutotestWithDialog" class="krasa.actions.RunAutotestWithDialog" text="RunAutotestWithDialog"
			description="RunAutotestWithDialog">
			<add-to-group group-id="EditorPopupMenu" anchor="first"/>
		</action>
		<action id="RunAutotest" class="krasa.actions.RunAutotest" text="RunAutotest" description="RunAutotest">
			<add-to-group group-id="EditorPopupMenu" anchor="first"/>
		</action>
		<action id="RunAutotestInIntelliJWithDialog" class="krasa.actions.RunAutotestInIntelliJWithDialog" text="Run Autotest In IntelliJ with Dialog" description="Run Autotest In IntelliJ with Dialog">
			<add-to-group group-id="EditorPopupMenu" anchor="first"/>
		</action>
		<action id="RunAutotestInIntelliJ" class="krasa.actions.RunAutotestInIntelliJ" text="Run Autotest In IntelliJ" description="Run Autotest In IntelliJ">
			<add-to-group group-id="EditorPopupMenu" anchor="first"/>
		</action>
	</actions>

	<extensions defaultExtensionNs="com.intellij">

		<toolWindow anchor="right" icon="AllIcons.Providers.Oracle" id="Autotest"
			factoryClass="krasa.toolwindow.AutotestToolWindowFactory" />

		<applicationService serviceInterface="krasa.model.AutotestState"
			serviceImplementation="krasa.model.AutotestState" />
	</extensions>
	<application-components>
		<component>
			<implementation-class>krasa.ProjectCommandLauncherComponent</implementation-class>
		</component>
	</application-components>
</idea-plugin>